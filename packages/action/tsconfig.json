{
  "compilerOptions": {
    "allowJs": true,
    "lib": ["ES2022", "dom"],
    "strict": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "jsxImportSource": "@builder.io/qwik"
  },
  "exclude": ["node_modules"]
}
